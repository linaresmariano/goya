doctype html
html(ng-app="APP")
	head
		link(rel='stylesheet', href='/stylesheets/style.css')
		link(rel='stylesheet', href='http://netdna.bootstrapcdn.com/bootstrap/3.0.3/css/bootstrap.min.css',media='screen')
		script(src='/js/lib/jquery.min.js')
		script(src='http://netdna.bootstrapcdn.com/bootstrap/3.0.3/js/bootstrap.min.js')
		script(src='/js/lib/jquery-ui.custom.min.js')
		title= title
	body
		.navbar.navbar-inverse.navbar-fixed-top(role='navigation')
				.navbar-header
					button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
						span.sr-only Toggle navigation
						span.icon-bar
						span.icon-bar
						span.icon-bar
					a.navbar-brand(href='#') Goya
						span.glyphicon.glyphicon-book.icon-main
				.collapse.navbar-collapse
					ul.nav.navbar-nav
						li.active
							a(href='../../') Home
						li.dropdown
							a.dropdown-toggle(href='#', data-toggle='dropdown') Altas
								span.caret
							ul.dropdown-menu(role='menu')
								li
									a(href='#') Curso
								li
									a(href='#') Aula
								li
									a(href='#') Profesor
								li
									a(href='../../subject/create') Materia
								li
									a(href='#') Carrera
						li.dropdown
							a.dropdown-toggle(href='#', data-toggle='dropdown') Vistas
								span.caret
							ul.dropdown-menu(role='menu')
								li
									a(href='../../grid/2014/1') Cursos
								li
									a(href='#') Aulas
						li.dropdown
							a.dropdown-toggle(href='#', data-toggle='dropdown') Reportes
								span.caret
							ul.dropdown-menu(role='menu')
								li
									a(href='#') Reporte 1
								li
									a(href='#') Reporte 2
						li
							a(href='#contact') Contact
		block content